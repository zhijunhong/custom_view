<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_goto_search_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转SearchView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_goto_radar_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转RadarView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_goto_search_view" />

    <Button
        android:id="@+id/btn_goto_bezier_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转BezierView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_goto_radar_view" />

</androidx.constraintlayout.widget.ConstraintLayout>